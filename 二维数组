#include<stdio.h>
#include<iostream>
using namespace std;
int main() {
	string b[3]= {"张三","李四","黄五"};
	int a[3][3]= {{85,52,99},{78,45,99},{60,70,80},};
	for(int i=0; i<3; i++) {
		int sum=0;
		for(int j=0; j<3; j++) {
			sum+=a[i][j];
		}
		cout<<b[i]<<"的总分为"<<sum<<endl;

	}
	system("pause");
	return 0;
}
