#include<iostream>
using namespace std;
int main(){
	int a = 10;
	cout << "请输入你对电影的评分：" << endl;
	cin >> a;
	switch (a) {
	case 10:
	case 9:
		cout << "佳作！" << endl;
		break;
	case 8:
	case 7:
	case 6:
		cout << "还行吧\n";
		break;
	default://指以上条件均不满足
		cout << "烂片\n";
		break;
	}

	
	system("pause");
	return 0;
}
