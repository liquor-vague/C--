#include<iostream>
using namespace std;
int main(){
	int a = 10;
	cout << "请输入你对电影的评分：" << endl;
	cin >> a;
	switch (a) {
	case 10:
	case 9:
		cout << "佳作！" << endl;
		break;
	case 8:
	case 7:
	case 6:
		cout << "还行吧\n";
		break;
	default://指以上条件均不满足
		cout << "烂片\n";
		break;
	}
//switch之后括号内的表达式只能是整数（byte，short，char和int）或字符型表达式，不能是长整型或其它任何类型。
	system("pause");
	return 0;
}
