#include<stdio.h>
#include<iostream>
using namespace std;
void swap(int num1,int num2) {
	cout<<"交换前num1的值为"<<num1<<endl;
	cout<<"交换前num2的值为"<<num2<<endl;
	int temp=num2;
	num2=num1;
	num1=temp;
	cout<<"交换后num1的值为"<<num1<<endl;
	cout<<"交换后num2的值为"<<num2<<endl;

}
int main() {
	int a=1;
	int b=2;
	cout<<"a的值为"<<a<<endl;
	cout<<"b的值为"<<b<<endl;
	swap(a,b);
	system("pause");
	return 0;
}
